			<param plane_axis="xaxis:RightArm"/><!-- Y axis -->
			<param cone_y="35" cone_z="40" cone_freedom="10"/>
			<param damping="1" spring_constant="200" min_restitution="0"/>
		</constraint>

		<constraint type="limited_hinge" name="RightForeArm" enabled="false">
