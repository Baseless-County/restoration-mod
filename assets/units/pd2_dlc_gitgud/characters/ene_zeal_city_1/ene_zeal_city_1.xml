<sequence_manager>
	<unit>
		<variables>
										
			
			<var_gadgets_flashlight value="0"/>					
			
			<!-- zeal swat -->
			<var_gadgets_city_light value="0"/>					
			
			<!-- goggle a -->
			<var_goggles value="0"/>					
		</variables>
	

				
		<body name="'visor'">
			<endurance explosion="4" bullet="1">
				<run_sequence name="'crk_visor'"/>
			</endurance>	
			<endurance explosion="6" >
				<run_sequence name="'destroy_altyn'"/>
			</endurance>
		</body>	

		<body name="'glass_swat'">
			<endurance explosion="4" bullet="1">
				<run_sequence name="'crk_goggles'"/>
			</endurance>
		</body>	
		
		<body name="'glass_c'">
			<endurance bullet="1" explosion="4">
				<run_sequence name="'crk_goggles'"/>
			</endurance>
		</body>	

		<filter name="'check_city_swat_ar'">
			<check value="vars.var_gadgets_city_light == 1"/>
		</filter>		
		<filter name="'check_city_swat_sg'">
			<check value="vars.var_gadgets_city_light == 2"/>
		</filter>
		<filter name="'check_city_swat_smg'">
			<check value="vars.var_gadgets_city_light == 3"/>
		</filter>
		<filter name="'check_city_shield'">
			<check value="vars.var_gadgets_city_light == 4"/>
		</filter>
		<filter name="'check_city_sniper'">
			<check value="vars.var_gadgets_city_light == 5"/>
		</filter>

		<filter name="'check_goggle_a'">
			<check value="vars.var_goggles == 1"/>
		</filter>

		<filter name="'check_flashlights_on_sg'">
			<check value="vars.var_gadgets_flashlight == 1"/>
			<check value="vars.var_gadgets_city_light == 2"/>
		</filter>

		<filter name="'check_flashlights_on_ar'">
			<check value="vars.var_gadgets_flashlight == 1"/>
			<check value="vars.var_gadgets_city_light == 1"/>
		</filter>

		<filter name="'check_goggle_b'">
			<check value="vars.var_goggles == 2"/>
		</filter>
		
		<filter name="'check_goggle_c'">
			<check value="vars.var_goggles == 3"/>
		</filter>
		
		<filter name="'check_goggle_d'">
			<check value="vars.var_goggles == 4"/>
		</filter>
		
		
		<sequence editable_state="true" name="'swat_ar'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="1"/>			
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_ar_helmetlight'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="1"/>
			<set_variables var_gadgets_flashlight="1"/>			
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_sg'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="2"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'swat_sg_nvg'" triggable="true" once="true">	
			<set_variables var_gadgets_city_light="2"/>		
			<set_variables var_gadgets_flashlight="1"/>
			<run_sequence name="'gadget_meat_grinder'"/>					
		</sequence>

		<sequence editable_state="true" name="'swat_smg'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="3"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_shield'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="4"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'swat_sniper'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="5"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'gadget_meat_grinder'" triggable="true" once="true">	
			
			<!-- rifleman -->
			<run_sequence name="'head_'..pick('balaclava', 'balaclava_b', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava_c', 'balaclava')"/>
			<object filter="'check_city_swat_ar'" enabled="true" name="'g_swat_ar'"/>						
			<object filter="'check_city_swat_ar'" enabled="true" name="'g_swat_ar_lod1'"/>
			
			<!-- extra attachments! :D -->
			<run_sequence filter="'check_city_swat_ar'" name="'helmet'..pick('_chops','_chops','_nun','_headset','_chops','_nun','_nun')"/>
		
			<!-- sg -->
			
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_swat_sg'"/>						
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_swat_sg_lod1'"/>
			
			<!-- extra attachments! :D -->
			
			<run_sequence filter="'check_city_swat_sg'" name="'helmet'..pick('_chops','_chops','_chops','_nun','_chops', '_chops')"/>

			<object filter="'check_city_swat_sg'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_city_swat_sg'" enabled="false" name="'g_helmet_lod2'"/>
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_helmet_sg_lod1'"/>						
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_helmet_sg_lod2'"/>
			
			<run_sequence filter="'check_city_swat_sg'" name="'helmet_nvg_up'"/>
		
			<!-- sg (nvgs on)-->
			<run_sequence filter="'check_flashlights_on_sg'" name="'helmet_nvg_down'"/>

			<!-- smg -->			
						
			<object filter="'check_city_swat_smg'" enabled="true" name="'g_swat_smg'"/>						
			<object filter="'check_city_swat_smg'" enabled="true" name="'g_swat_smg_lod1'"/>
			
			<object filter="'check_city_swat_smg'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_city_swat_smg'" enabled="false" name="'g_helmet_lod2'"/>
			
			<run_sequence filter="'check_city_swat_smg'" name="'hat_'..pick('b','b','b','b','a','a','b')"/>
			
			<decal_mesh filter="'check_city_swat_smg'" enabled="false" name="'dm_helmet'"/>					
			
			<run_sequence filter="'check_city_swat_smg'" name="'holster_'..pick('r','r','r','r', 'nothing','nothing')"/>

			<run_sequence filter="'check_city_swat_smg'" name="'helmet'..pick('_headset_ng', '_headset_smg','_headset_ng','_nun_smg', '_headset_smg', '_headset_smg')"/>

			<!-- shield -->			
			<object filter="'check_city_shield'" enabled="true" name="'g_swat_shield'"/>						
			<object filter="'check_city_shield'" enabled="true" name="'g_swat_shield_lod1'"/>
			
			
			<run_sequence filter="'check_city_shield'" name="'helmet_shield'"/>
			
			<run_sequence filter="'check_city_shield'" name="'holster_'..pick('r_shield','l_shield')"/>
			
						
			<!-- sniper -->			
			<object filter="'check_city_sniper'" enabled="true" name="'g_body_sniper'"/>						
			<object filter="'check_city_sniper'" enabled="true" name="'g_body_sniper_lod1'"/>
			<object filter="'check_city_sniper'" enabled="false" name="'g_body_lod1'"/>						
			<object filter="'check_city_sniper'" enabled="false" name="'g_body'"/>
			<object filter="'check_city_sniper'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_city_sniper'" enabled="false" name="'g_helmet_lod2'"/>
			<run_sequence filter="'check_city_sniper'" name="'hat_'..pick('b','b','b','b','a','a','b')"/>
			<run_sequence filter="'check_city_sniper'" name="'helmet'..pick('_headset_ng', '_headset_smg','_headset_ng','_nun_smg', '_headset_smg', '_headset_smg')"/>			
			<decal_mesh filter="'check_city_sniper'" enabled="false" name="'dm_helmet'"/>					
			
		</sequence>

		<sequence editable_state="true" name="'crk_visor'" once="true" triggable="true">
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod9'"/>
			<object filter="'check_city_shield'" enabled="true" name="'g_helmet_lod10'"/>
			<effect filter="'check_city_shield'" damp_time="5" name="'effects/particles/bullet_hit/glass_breakable/bullet_hit_glass_breakable'" parent="object( 'Head' )" position="v()"/>
		</sequence>				

		<sequence editable_state="true" name="'destroy_altyn'" once="true" triggable="true">
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod7'"/>
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod8'"/>
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod9'"/>
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod10'"/>
			
			<body 			filter="'check_city_shield'" enabled="false" name="'visor'" />
			<decal_mesh 	filter="'check_city_shield'" enabled="false" name="'c_visor'" />
			
			<effect filter="'check_city_shield'" damp_time="4" name="'effects/payday2/particles/impacts/metal_impact_pd2'" parent="object( 'Head' )" position="v()"/>			
			<effect filter="'check_city_shield'" damp_time="5" name="'effects/payday2/particles/impacts/metal_impact_pd2'" parent="object( 'Head' )" position="v()"/>
			
			<sound action="'play'" event="'swatturret_weakspot_hit'" object="'Head'"/>					
		</sequence>

		<sequence editable_state="true" name="'hat_a'" once="true" triggable="true">
			<object enabled="true" name="'g_hat_lod1'"/>
			<object enabled="true" name="'g_hat_lod2'"/>
		</sequence>
		<sequence editable_state="true" name="'hat_b'" once="true" triggable="true">
			<object enabled="true" name="'g_hat_lod3'"/>
			<object enabled="true" name="'g_hat_lod4'"/>
		</sequence>
		

		<sequence editable_state="true" name="'goggles_a'" once="true" triggable="true">
			<object enabled="true" name="'g_goggles_a'"/>
			<object enabled="true" name="'g_goggles_a_lod1'"/>
			<object enabled="true" name="'g_glass'"/>
			<set_variables var_goggles="1"/>					
			<decal_mesh enabled="true" name="'c_glass'" />					
			<body enabled="true" name="'glass_swat'" />										
		</sequence>
		
		<sequence editable_state="true" name="'goggles_b'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod3'"/>
			<object enabled="true" name="'g_helmet_lod4'"/>
			<object enabled="true" name="'g_glass'"/>
			
			<set_variables var_goggles="2"/>										
			<decal_mesh enabled="true" name="'c_glass'" />					
			<body enabled="true" name="'glass_swat'" />										
		</sequence>
		
		<sequence editable_state="true" name="'goggles_c'" once="true" triggable="true">
			<object enabled="true" name="'g_goggles_c'"/>
			<object enabled="true" name="'g_goggles_c_lod1'"/>
			<object enabled="true" name="'g_glass_c'"/>
			<set_variables var_goggles="3"/>										
			<decal_mesh enabled="true" name="'c_glass_c'" />					
			<body enabled="true" name="'glass_c'" />										
		</sequence>
		
		<sequence editable_state="true" name="'goggles_e'" once="true" triggable="true">
			<object enabled="true" name="'g_glasses'"/>
			<object enabled="true" name="'g_glasses_glass'"/>
			<set_variables var_goggles="5"/>										
		</sequence>
		
		<sequence editable_state="true" name="'crk_goggles'" once="true" triggable="true">
			<object filter="'check_goggle_a'" enabled="true" name="'g_glass_crk'"/>
			<object filter="'check_goggle_a'" enabled="false" name="'g_glass'"/>
			
			<object filter="'check_goggle_b'" enabled="true" name="'g_glass_crk'"/>
			<object filter="'check_goggle_b'" enabled="false" name="'g_glass'"/>
			
			<object filter="'check_goggle_c'" enabled="false" name="'g_glass_c'"/>
			<object filter="'check_goggle_c'" enabled="true" name="'g_glass_c_crk'"/>					
		</sequence>				
		
		<sequence editable_state="true" name="'helmet_headset'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a','a','c','a','b','b','e','a','a')"/>								
		
			<object enabled="true" name="'g_headset'"/>
			<object enabled="true" name="'g_headset_lod1'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_headset_smg'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a','a','c','a','e','a','a')"/>								
		
			<object enabled="true" name="'g_headset'"/>
			<object enabled="true" name="'g_headset_lod1'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_headset_ng'" once="true" triggable="true">		
			<object enabled="true" name="'g_headset'"/>
			<object enabled="true" name="'g_headset_lod1'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_nun'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a','a','c','a','b','b','e','a','a')"/>								
		</sequence>
		
		<sequence editable_state="true" name="'helmet_nun_smg'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a','a','c','a','e','a','a')"/>								
		</sequence>
		
		<sequence editable_state="true" name="'helmet_chops'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a','a','c','a','b','b','e','a','a')"/>								
		
			<object enabled="true" name="'g_helmet_lod5'"/>
			<object enabled="true" name="'g_helmet_lod6'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_nvg_up'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_sg_lod3'"/>
			<object enabled="true" name="'g_helmet_sg_lod4'"/>
		</sequence>

		<sequence editable_state="true" name="'helmet_shield'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod7'"/>
			<object enabled="true" name="'g_helmet_lod8'"/>			
			<object enabled="true" name="'g_helmet_lod9'"/>			

			<body enabled="true" name="'visor'" />
			<decal_mesh enabled="true" name="'c_visor'" />				
		</sequence>


		<sequence editable_state="true" name="'mask_glass'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod17'"/>
		</sequence>
				
		<sequence editable_state="true" name="'helmet_nvg_down'" once="true" triggable="true">
			<object enabled="false" name="'g_helmet_sg_lod3'"/>
			<object enabled="false" name="'g_helmet_sg_lod4'"/>		
			<object enabled="true" name="'g_helmet_sg_lod5'"/>
			<object enabled="true" name="'g_helmet_sg_lod6'"/>
		</sequence>
		

		<sequence editable_state="true" name="'spawn_helmet'" once="true" triggable="true">
			<decal_mesh filter="'check_goggle_b'" enabled="false" name="'c_glass'" />										
			<body filter="'check_goggle_b'" enabled="false" name="'glass_swat'" />	
			<object filter="'check_goggle_b'"  enabled="false" name="'g_glass'"/>
			<object filter="'check_goggle_b'"  enabled="false" name="'g_glass_crk'"/>
	
			<object enabled="false" name="'g_helmet_lod5'"/>
			<object enabled="false" name="'g_helmet_lod6'"/>
	
			<body filter="'check_city_shield'" enabled="false" name="'glass_swat'" />	
			<decal_mesh filter="'check_city_shield'" enabled="false" name="'c_glass'" />													
									
			<body filter="'check_city_shield'" enabled="false" name="'visor'" />	
			<decal_mesh filter="'check_city_shield'" enabled="false" name="'c_visor'" />													

			<run_sequence filter="'check_city_shield'" name="'destroy_altyn'"/>			

			<effect_spawner enabled="false" name="'es_flashlight'"/>			
		</sequence>		

		<!-- BALACLAVA W/ "INSIDE" FACE SEQUENCES -->
		<sequence editable_state="true" name="'head_balaclava'" triggable="true" once="true">	
			<object enabled="true" name="'g_balaclava_a'"/>
			<object enabled="true" name="'g_balaclava_a_lod1'"/>	

			<run_sequence name="'face_'..pick('a','b','c','d','e')" />										
		</sequence>	
		
		<sequence editable_state="true" name="'head_balaclava_b'" triggable="true" once="true">	
			<object enabled="true" name="'g_balaclava_b'"/>
			<object enabled="true" name="'g_balaclava_b_lod1'"/>	

			<run_sequence name="'face_'..pick('a','b','c','d','e')" />										
		</sequence>								
		
		<sequence editable_state="true" name="'head_balaclava_c'" triggable="true" once="true">	
			<object enabled="true" name="'g_balaclava_c'"/>
			<object enabled="true" name="'g_balaclava_c_lod1'"/>	

			<run_sequence name="'exposed_head'..pick('_a','_b','_c')" />										
		</sequence>								

		<!-- RANDOM HOLSTERS -->

		<sequence editable_state="true" name="'holster_r'" once="true" triggable="true">
			<object enabled="true" name="'g_holster_r_hip'"/>
			<object enabled="true" name="'g_holster_r_hip_lod1'"/>
		</sequence>								
		<!-- RANDOM HOLSTERS end -->
		
		<sequence editable_state="true" name="'face_a'" once="true" triggable="true">
			<object enabled="true" name="'g_face_a'"/>
			<object enabled="true" name="'g_face_a_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'face_b'" once="true" triggable="true">
			<object enabled="true" name="'g_face_b'"/>
			<object enabled="true" name="'g_face_b_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'face_c'" once="true" triggable="true">
			<object enabled="true" name="'g_face_c'"/>
			<object enabled="true" name="'g_face_c_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'face_d'" once="true" triggable="true">
			<object enabled="true" name="'g_face_d'"/>
			<object enabled="true" name="'g_face_d_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'face_e'" once="true" triggable="true">
			<object enabled="true" name="'g_face_e'"/>
			<object enabled="true" name="'g_face_e_lod1'"/>
		</sequence>	
		<!-- END BALACALVA FACES -->

		<!-- EXPOSED HEAD SEQUENCES -->				
		<sequence editable_state="true" name="'exposed_head_a'" once="true" triggable="true">
			<object enabled="true" name="'g_head_a'"/>
			<object enabled="true" name="'g_head_a_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'exposed_head_b'" once="true" triggable="true">
			<object enabled="true" name="'g_head_b'"/>
			<object enabled="true" name="'g_head_b_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'exposed_head_c'" once="true" triggable="true">
			<object enabled="true" name="'g_head_c'"/>
			<object enabled="true" name="'g_head_c_lod1'"/>
		</sequence>	
	
		<sequence editable_state="true" name="'disable_hitbox'" triggable="true">
			<body enabled="false" name="'body'"/>
			<body enabled="false" name="'Head'"/>
			<body enabled="false" name="'rag_LeftLeg'"/>
			<body enabled="false" name="'rag_RightLeg'"/>
			<body enabled="false" name="'rag_RightUpLeg'"/>
			<body enabled="false" name="'rag_LeftUpLeg'"/>
		</sequence>
		<sequence editable_state="true" name="'leg_arm_hitbox'" triggable="true">
			<body enabled="true" name="'body'"/>
			<body enabled="true" name="'Head'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Hips'"/>
			<body enabled="true" motion="'keyframed'" name="'rag_LeftUpLeg'"/>
			<body enabled="true" motion="'keyframed'" name="'rag_RightUpLeg'"/>
			<body enabled="true" motion="'keyframed'" name="'rag_LeftLeg'"/>
			<body enabled="true" motion="'keyframed'" name="'rag_RightLeg'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Spine'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Spine1'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Spine2'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_LeftForeArm'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_RightForeArm'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_LeftArm'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_RightArm'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Head'"/>
			<constraint enabled="false" name="'LeftUpLeg'"/>
			<constraint enabled="false" name="'RightUpLeg'"/>
			<constraint enabled="false" name="'LeftLeg'"/>
			<constraint enabled="false" name="'RightLeg'"/>
			<constraint enabled="false" name="'Spine'"/>
			<constraint enabled="false" name="'Spine1'"/>
			<constraint enabled="false" name="'Spine2'"/>
			<constraint enabled="false" name="'LeftForeArm'"/>
			<constraint enabled="false" name="'RightForeArm'"/>
			<constraint enabled="false" name="'LeftArm'"/>
			<constraint enabled="false" name="'RightArm'"/>
			<constraint enabled="false" name="'Head'"/>
		</sequence>
		<sequence editable_state="true" name="'prepare_ragdoll'" triggable="true">
			<body enabled="false" motion="'dynamic'" name="'rag_Hips'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_LeftUpLeg'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_RightUpLeg'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_LeftLeg'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_RightLeg'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_Spine'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_Spine1'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_Spine2'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_LeftForeArm'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_RightForeArm'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_LeftArm'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_RightArm'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_Head'"/>
		</sequence>
		<sequence editable_state="true" name="'switch_to_ragdoll'" triggable="true">
			<body enabled="false" name="'body'"/>
			<body enabled="false" name="'Head'"/>
			<body enabled="true" motion="'dynamic'" name="'rag_Hips'"/>
			<body enabled="true" motion="'dynamic'" name="'rag_LeftUpLeg'"/>
			<body enabled="true" motion="'dynamic'" name="'rag_RightUpLeg'"/>
			<body enabled="true" motion="'dynamic'" name="'rag_LeftLeg'"/>
			<body enabled="true" motion="'dynamic'" name="'rag_RightLeg'"/>
			<body enabled="true" motion="'dynamic'" name="'rag_Spine'"/>
			<body enabled="true" motion="'dynamic'" name="'rag_Spine1'"/>
			<body enabled="true" motion="'dynamic'" name="'rag_Spine2'"/>
			<body enabled="true" motion="'dynamic'" name="'rag_LeftForeArm'"/>
